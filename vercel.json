{
  "version": 2,
  "builds": [
    { "src": "examples/server/server/**/*", "use": "@vercel/static" },
    { "src": "api/index.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.py" },
    { "src": "/(.*)", "dest": "/examples/server/server/$1" },
    { "src": "/", "dest": "/examples/server/server/index.html" }
  ],
  "functions": {
    "api/index.py": {
      "maxDuration": 30
    }
  }
}