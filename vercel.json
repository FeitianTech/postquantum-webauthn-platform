{
  "version": 2,
  "builds": [
    { "src": "examples/server/server/static/**/*", "use": "@vercel/static" },
    { "src": "api/server.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/server.py" },
    { "src": "/(.*)", "dest": "examples/server/server/static/$1" },
    { "src": "/", "dest": "examples/server/server/static/index.html" }
  ],
  "functions": {
    "api/server.py": {
      "maxDuration": 30
    }
  }
}